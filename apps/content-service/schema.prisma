datasource db {
    url = env("DATABASE_URL")
    provider = "postgresql"
}

generator client {
    provider = "prisma-client-js"
    output = "./src/prisma/generated"
    binaryTargets = ["native", "linux-arm64-openssl-1.1.x"]
}

model Application {
  id       Int      @id @default(autoincrement())
}
